# Copy this file to `.env` and replace values with your Neon connection info
# Example Neon connection string (replace placeholders below):
DATABASE_URL=postgresql://<USER>:<PASSWORD>@<HOST>:5432/<DBNAME>?sslmode=require&channel_binding=require

# Optional
PORT=8080
# Pool tuning (optional)
# DB_POOL_MAX=5
# DB_POOL_MIN=0
# DB_POOL_ACQUIRE=30000
# DB_POOL_IDLE=10000
